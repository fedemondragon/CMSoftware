procedure TFormMainMenu.SpeedButtonSalirClick(Sender: TObject);
var
  Cerrar:Integer;
 Begin
  Cerrar:=Application.MessageBox('¿Desea Salir del programa?','¡Confirmando!',MB_YESNO);
        Begin
         if Cerrar=IDYES then
           close;
        End;
  End;



